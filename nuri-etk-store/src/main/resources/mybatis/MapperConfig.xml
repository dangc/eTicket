<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <properties>
        <property name="prop.dt" value="%Y-%m-%d %H:%i:%s"></property>
        <property name="prop.small.dt" value="%Y-%m-%d %H:%i"></property>
        <property name="prop.date" value="%Y-%m-%d"></property>
        <property name="prop.time" value="%H:%i:%s"></property>
        <property name="prop.dt.plain" value="%Y%m%d%H%i%s"></property>
    </properties>

    <typeAliases>
        <typeAlias alias="dcuJpo" type="com.nuri.etk.store.jpo.DcuJpo"/>
        <typeAlias alias="meterJpo" type="com.nuri.etk.store.jpo.MeterJpo"/>
        <typeAlias alias="customerJpo" type="com.nuri.etk.store.jpo.CustomerJpo"/>
        <typeAlias alias="receiptJpo" type="com.nuri.etk.store.jpo.ReceiptJpo"/>
        <typeAlias alias="paymentInfoJpo" type="com.nuri.etk.store.jpo.PaymentInfoJpo"/>
        <typeAlias alias="customerInfoJpo" type="com.nuri.etk.store.jpo.CustomerInfoJpo"/>
        <typeAlias alias="debtInfoJpo" type="com.nuri.etk.store.jpo.DebtInfoJpo"/>
        <typeAlias alias="usageJpo" type="com.nuri.etk.store.jpo.UsageJpo"/>
        <typeAlias alias="usageBalanceJpo" type="com.nuri.etk.store.jpo.UsageBalanceJpo"/>
        <typeAlias alias="dailyUsagesJpo" type="com.nuri.etk.store.jpo.DailyUsagesJpo"/>
        <typeAlias alias="monthlyUsagesJpo" type="com.nuri.etk.store.jpo.MonthlyUsagesJpo"/>
    </typeAliases>

    <mappers>
        <!--<mapper resource="mybatis/oracle/DcuMapper.xml" />-->
        <mapper resource="mybatis/oracle/MeterMapper.xml" />
        <mapper resource="mybatis/oracle/CustomerMapper.xml" />
        <mapper resource="mybatis/oracle/ReceiptMapper.xml" />
        <mapper resource="mybatis/oracle/ChargeHistoryMapper.xml" />
        <mapper resource="mybatis/oracle/DebtMapper.xml" />
        <mapper resource="mybatis/oracle/UsageMapper.xml" />
        <!--<mapper resource="mybatis/oracle/ChargeMapper.xml" />-->
    </mappers>

</configuration>
